{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "alwaysShowTabs": false,
    "defaultProfile": "{00c71c6f-3539-4363-af27-ce2b738251a8}",
    "disableAnimations": true,
    "initialCols": 160,
    "initialRows": 40,
    "keybindings": [
        {
            "command": {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "alt+shift+j"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "alt+shift+h"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "alt+shift+l"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "alt+shift+k"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "horizontal"
            },
            "keys": "alt+shift+s"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "vertical"
            },
            "keys": "alt+shift+v"
        },
        {
            "command": {
                "action": "splitPane",
                "profile": "Windows PowerShell",
                "split": "vertical"
            },
            "keys": "alt+shift+w"
        },
        {
            "command": "closePane",
            "keys": "alt+shift+d"
        },
        {
            "command": "openSettings",
            "keys": "alt+shift+e"
        }
    ],
    "launchMode": "default",
